{
  "name": "@bbp/react-nexus",
  "version": "1.1.6",
  "description": "A set of React components for Nexus",
  "keywords": [
    "react",
    "nexus",
    "javascript",
    "typescript"
  ],
  "author": "Julien Machon <julien.machon@epfl.ch>",
  "homepage": "https://github.com/BlueBrain/nexus-js#readme",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "unpkg": "./dist/index.js",
  "module": "./es/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "dist": "dist",
    "es": "es",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlueBrain/nexus-js.git"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "clean": "rm -fr es/ && rm -fr dist/ && rm -fr lib/"
  },
  "bugs": {
    "url": "https://github.com/BlueBrain/nexus-js/issues"
  },
  "dependencies": {
    "@bbp/nexus-link": "^1.1.6",
    "ts-invariant": "0.4.1"
  },
  "peerDependencies": {
    "@bbp/nexus-sdk": ">= 1.1.6",
    "react": ">= 16.8.3"
  },
  "devDependencies": {
    "@bbp/nexus-sdk": ">= 1.1.6",
    "@types/react": "^16.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
